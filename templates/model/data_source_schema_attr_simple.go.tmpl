schemaAttrs["{{ .TFName }}"] = {{ .Type.DSSchemaTemplateData.Type }}{
{{- if .Type.DSSchemaTemplateData.Required }}
    Required: recurse,
    Computed: !recurse,
{{- template "common_schema_attr_validators.go.tmpl" . }}
